#include <iostream>
using namespace std;

int main(){
    int n, m;
    cout << "Tamaño del vector A: ";
    cin >> n;
    cout << "Tamaño del vector B: ";
    cin >> m;

    int A[100], B[100], C[200];

    for(int i = 0; i < n; i++){
        cin >> A[i];
    }
    for(int i = 0; i < m; i++){
        cin >> B[i];
    }

    for(int i = 0; i < n; i++){
        C[i] = A[i];
    }

    for(int i = 0; i < m; i++){
        C[n+i] = B[i];
    }

    cout << "Vector concatenado: ";
    for(int i = 0; i < n+m; i++){
        cout << C[i] << " ";
    }

    return 0;
}
