#include <iostream>
using namespace std;

int main() {
    int n, m;

    // tamaño de A
    cout << "Cuantos elementos tiene el vector A? ";
    cin >> n;

    int A[100];

    cout << "Ingrese los elementos de A:" << endl;
    for (int i = 0; i < n; i++) {
        cout << "A[" << i << "]: ";
        cin >> A[i];
    }

    // tamaño de B
    cout << "Cuantos elementos tiene el vector B? ";
    cin >> m;

    int B[100];

    cout << "Ingrese los elementos de B:" << endl;
    for (int i = 0; i < m; i++) {
        cout << "B[" << i << "]: ";
        cin >> B[i];
    }

    // concatenar en C
    int C[200];

    // primero copiamos A
    for (int i = 0; i < n; i++) {
        C[i] = A[i];
    }

    // luego copiamos B
    for (int j = 0; j < m; j++) {
        C[n + j] = B[j];
    }

    // mostrar resultado
    cout << "Vector concatenado C: ";
    for (int i = 0; i < n + m; i++) {
        cout << C[i] << " ";
    }

    return 0;
}
