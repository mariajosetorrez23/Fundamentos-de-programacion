#include <iostream>
#include <vector>
#include <algorithm>  // sort
using namespace std;

int main() {
    int n;
    cout << "Cantidad de elementos: ";
    cin >> n;

    vector<double> v(n);
    cout << "Ingrese los valores:\n";
    for (int i = 0; i < n; ++i) cin >> v[i];

    sort(v.begin(), v.end());  // ascendente por defecto

    cout << "Vector ordenado:\n";
    for (double x : v) cout << x << " ";
    cout << endl;
    return 0;
}
