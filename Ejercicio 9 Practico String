#include <iostream>
using namespace std;

int main(){
    char A[20], B[20];
    int nA, nB;

    cout << "Cantidad de elementos en A: ";
    cin >> nA;
    cout << "Ingrese elementos de A:\n";
    for(int i = 0; i < nA; i++){
        cin >> A[i];
    }

    cout << "Cantidad de elementos en B: ";
    cin >> nB;
    cout << "Ingrese elementos de B:\n";
    for(int i = 0; i < nB; i++){
        cin >> B[i];
    }

    cout << "Interseccion: { ";

    for(int i = 0; i < nA; i++){
        for(int j = 0; j < nB; j++){
            if(A[i] == B[j]){
                cout << A[i] << " ";
                break;
            }
        }
    }

    cout << "}";
    return 0;
}
