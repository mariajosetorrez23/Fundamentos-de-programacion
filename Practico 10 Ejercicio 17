#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n;
    cout << "Cantidad de numeros de Fibonacci: ";
    cin >> n;

    if (n <= 0) return 0;

    vector<long long> fib(n);
    fib[0] = 0;
    if (n > 1) fib[1] = 1;

    for (int i = 2; i < n; ++i)
        fib[i] = fib[i-1] + fib[i-2];

    cout << "Fibonacci:\n";
    for (long long x : fib) cout << x << " ";
    cout << endl;
    return 0;
}
