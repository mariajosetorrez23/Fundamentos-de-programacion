#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n;
    cout << "Cantidad de elementos: ";
    cin >> n;

    vector<int> v(n);
    cout << "Ingrese los valores:\n";
    for (int i = 0; i < n; ++i) cin >> v[i];

    for (int i = 0; i < n / 2; ++i) {
        int tmp = v[i];
        v[i] = v[n - 1 - i];
        v[n - 1 - i] = tmp;
    }

    cout << "Vector invertido:\n";
    for (int x : v) cout << x << " ";
    cout << endl;
    return 0;
}
