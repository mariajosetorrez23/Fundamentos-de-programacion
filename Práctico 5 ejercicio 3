#include <stdio.h>

int main() {
    int N, u, d, c, m;

    printf("Ingrese un n√∫mero (menor que 10000): ");
    scanf("%d", &N);

    u = N % 10;
    d = (N / 10) % 10;
    c = (N / 100) % 10;
    m = (N / 1000) % 10;

    printf("Unidades: %d\nDecenas: %d\nCentenas: %d\nUnidades de mil: %d\n", u, d, c, m);
    return 0;
}
