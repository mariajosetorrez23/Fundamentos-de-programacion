#include <iostream>
using namespace std;

int main() {
    int n, m;

    cout << "Tamano del vector A: ";
    cin >> n;
    cout << "Tamano del vector B: ";
    cin >> m;

    int A[100], B[100], C[200];

    // Leer A
    cout << "Ingrese los valores de A:\n";
    for (int i = 0; i < n; i++) {
        cin >> A[i];
    }

    // Leer B
    cout << "Ingrese los valores de B:\n";
    for (int i = 0; i < m; i++) {
        cin >> B[i];
    }

    // ORDENAR A (metodo burbuja simple)
    for (int i = 0; i < n - 1; i++) {
        for (int j = 0; j < n - i - 1; j++) {
            if (A[j] > A[j + 1]) {
                int aux = A[j];
                A[j] = A[j + 1];
                A[j + 1] = aux;
            }
        }
    }

    // ORDENAR B
    for (int i = 0; i < m - 1; i++) {
        for (int j = 0; j < m - i - 1; j++) {
            if (B[j] > B[j + 1]) {
                int aux = B[j];
                B[j] = B[j + 1];
                B[j + 1] = aux;
            }
        }
    }

    // INTERCALAR (merge)
    int i = 0, j = 0, k = 0;

    while (i < n && j < m) {
        if (A[i] < B[j]) {
            C[k] = A[i];
            i++;
        } else {
            C[k] = B[j];
            j++;
        }
        k++;
    }

    // Copiar lo que quede de A
    while (i < n) {
        C[k] = A[i];
        i++;
        k++;
    }

    // Copiar lo que quede de B
    while (j < m) {
        C[k] = B[j];
        j++;
        k++;
    }

    // Mostrar resultado final
    cout << "Vector intercalado: ";
    for (int x = 0; x < k; x++) {
        cout << C[x] << " ";
    }

    return 0;
}
