#include <iostream>
using namespace std;

int main(){
    int n, m;
    cout << "Tamano de A: ";
    cin >> n;
    cout << "Tamano de B: ";
    cin >> m;

    int A[100], B[100], C[200];

    for(int i = 0; i < n; i++){
        cin >> A[i];
    }
    for(int i = 0; i < m; i++){
        cin >> B[i];
    }

    int i = 0, j = 0, k = 0;

    while(i < n && j < m){
        if(A[i] < B[j]){
            C[k] = A[i];
            i++;
        } else {
            C[k] = B[j];
            j++;
        }
        k++;
    }

    while(i < n){
        C[k] = A[i];
        i++;
        k++;
    }

    while(j < m){
        C[k] = B[j];
        j++;
        k++;
    }

    cout << "Vector intercalado: ";
    for(int x = 0; x < n+m; x++){
        cout << C[x] << " ";
    }

    return 0;
}
