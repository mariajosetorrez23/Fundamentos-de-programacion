#include <iostream>
using namespace std;

int main(){
    int n;
    cout << "Cantidad de elementos: ";
    cin >> n;

    int v[100];
    for(int i = 0; i < n; i++){
        cin >> v[i];
    }

    for(int i = 0; i < n/2; i++){
        int temp = v[i];
        v[i] = v[n-1-i];
        v[n-1-i] = temp;
    }

    cout << "Vector invertido: ";
    for(int i = 0; i < n; i++){
        cout << v[i] << " ";
    }

    return 0;
}
