#include <stdio.h>

int main() {
    float a, b, c, d, e, f, det, x, y;

    printf("Ingrese a, b, c, d, e, f (ax+by=c, dx+ey=f): ");
    scanf("%f %f %f %f %f %f", &a, &b, &c, &d, &e, &f);

    det = a*e - b*d;

    if (det == 0)
        printf("No hay solución única\n");
    else {
        x = (c*e - b*f) / det;
        y = (a*f - c*d) / det;
        printf("x = %.2f, y = %.2f\n", x, y);
    }
    return 0;
}
