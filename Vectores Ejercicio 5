#include <iostream>
using namespace std;

int main(){
    int n;
    cout << "Cantidad de elementos: ";
    cin >> n;

    int v[100];
    for(int i = 0; i < n; i++){
        cin >> v[i];
    }

    bool capicua = true;

    for(int i = 0; i < n/2; i++){
        if(v[i] != v[n-1-i]){
            capicua = false;
            break;
        }
    }

    if(capicua)
        cout << "Es capicua";
    else
        cout << "No es capicua";

    return 0;
}
