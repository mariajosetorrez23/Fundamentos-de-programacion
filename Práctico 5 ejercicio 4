#include <stdio.h>

int main() {
    int h1, m1, s1, h2, m2, s2;
    int dif_h, dif_m, dif_s;

    printf("Ingrese el primer horario (hh mm ss): ");
    scanf("%d %d %d", &h1, &m1, &s1);
    printf("Ingrese el segundo horario (hh mm ss): ");
    scanf("%d %d %d", &h2, &m2, &s2);

    dif_s = s2 - s1;
    dif_m = m2 - m1;
    dif_h = h2 - h1;

    if (dif_s < 0) { dif_s += 60; dif_m--; }
    if (dif_m < 0) { dif_m += 60; dif_h--; }

    printf("Tiempo transcurrido: %d:%d:%d\n", dif_h, dif_m, dif_s);
    return 0;
}
