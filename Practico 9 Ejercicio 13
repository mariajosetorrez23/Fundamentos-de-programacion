#include <iostream>
#include <cmath>
using namespace std;

double REDONDEAR(double R, int n) {
    double factor = pow(10, n);
    return round(R * factor) / factor;
}

int main() {
    double R;
    int n;
    cout << "Ingrese un numero real: ";
    cin >> R;
    cout << "Ingrese cantidad de decimales: ";
    cin >> n;
    cout << "Numero redondeado: " << REDONDEAR(R, n) << endl;
    return 0;
}
