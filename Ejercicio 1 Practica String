#include <iostream>
#include <string>
#include <ctype.h>
using namespace std;

int main() {
    string texto;
    cout << "Ingrese un texto: ";
    getline(cin, texto);

    for(int i = 0; i < texto.size(); i++){
        texto[i] = toupper(texto[i]);
    }

    cout << "Resultado: " << texto;
    return 0;
}
